! Generation of ephemerides

ephem.
	.objects	INT	! objects for which ephemeris is done (1/2/3)
	.epoch.start	TIM	! starting epoch for ephemeris
	.epoch.end	TIM	! ending epoch for ephemeris
	.step		REA	! ephemeris stepsize (d)
	.obscode	INT	! MPC observatory code (default=500)
	.timescale	CHA	! timescale of ephemeris (default=TDT)
	.fields		CHA	! output fields

ephem.appmot.			! output of apparent motion
	.format		CHA	! format
$I  'rectangular'	1	! RA/DEC components
$I  'polar'		2	! size and position angle
! units for velocity in the form A/T, where A is the angular unit (rad/deg/arcmin/arcsec)
! and T is the time unit (d/h/min/s), for instance: 'deg/d' or 'arcsec/min'
	.units		CHA	! units for velocity
